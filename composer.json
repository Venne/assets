{
	"name": "venne/assets",
	"description": "Venne assets component",
	"keywords": ["nette", "venne", "assets"],
	"homepage": "http://venne.cz",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Josef Kříž",
			"homepage": "http://josef-kriz.cz"
		}
	],
	"support": {
		"email": "info@venne.cz",
		"issues": "https://github.com/Venne/assets/issues"
	},
	"require": {
		"php": ">=5.4.0",
		"nette/nette": "~2.1.0",
		"janmarek/webloader": "~2.2.0"
	},
	"require-dev": {
		"nette/tester": "~1.1.0"
	},
	"suggest": {
		"venne/packages": ""
	},
	"autoload": {
		"psr-4": {
			"Venne\\Assets\\": "src/Assets/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"VenneTests\\": "tests/src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.0-dev"
		},
		"venne": {
			"configuration": {
				"extensions": {
					"assets": "Venne\\Assets\\DI\\AssetsExtension"
				}
			}
		}
	}
}